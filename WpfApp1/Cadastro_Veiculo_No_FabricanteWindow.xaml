<Window x:Class="WpfApp1.Cadastro_Veiculo_No_FabricanteWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="Cadastro_Veiculo_No_FabricanteWindow" Height="270" Width="400">
    <Grid>
        <ListBox Name="listVeiculos" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,10,0,0" Width="370" Height="90"/>
        <ListBox Name="listFabricantes" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,110,0,0" Width="370" Height="90" SelectionChanged="listFabricantes_SelectionChanged"/>
        <Button Click="ListarClick" Content="Listar" HorizontalAlignment="Left" Margin="10,205,0,0" VerticalAlignment="Top"/>
        <Button Click="CadastrarClick" Content="Cadastrar" HorizontalAlignment="Left" Margin="47,205,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
